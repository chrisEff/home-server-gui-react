{
	"name": "home-server-gui-react",
	"version": "1.0.0",
	"description": "A React GUI for my home automation API.",
	"scripts": {
		"build": "webpack --env=prod",
		"build-dev": "webpack --env=dev",
		"watch": "webpack -w --env=dev",
		"test": "jest",
		"test:update": "jest --updateSnapshot",
		"cover": "jest --coverage --collectCoverageFrom=src/**/*",
		"coveralls": "jest --coverage --collectCoverageFrom=src/**/* --coverageReporters=text-lcov | coveralls",
		"eslint": "eslint --ext js,jsx src test",
		"eslint-fix": "eslint --ext js,jsx src test --fix"
	},
	"keywords": [],
	"author": "chrisEff",
	"license": "MIT",
	"homepage": "https://chriseff.github.io/home-server-gui-react/",
	"repository": {
		"type": "git",
		"url": "git@github.com:chrisEff/home-server-gui-react.git"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/plugin-transform-runtime": "^7.10.4",
		"@babel/polyfill": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/runtime": "^7.10.4",
		"@chriseff/eslint-config": "^2.1.0",
		"@types/jest": "^26.0.4",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^26.1.0",
		"babel-loader": "^8.1.0",
		"coveralls": "^3.1.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"enzyme-to-json": "^3.5.0",
		"eslint": "^7.4.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-standard": "^4.0.1",
		"fetch-mock": "^9.10.3",
		"husky": "^4.2.5",
		"jest": "^26.1.0",
		"lint-staged": "^10.2.11",
		"node-fetch": "^2.6.0",
		"redux-mock-store": "^1.5.4",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-merge": "^5.0.8"
	},
	"dependencies": {
		"aphrodite": "^2.4.0",
		"dateformat": "^3.0.3",
		"lodash.sortby": "^4.7.0",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.2.0",
		"recharts": "^1.8.5",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0"
	},
	"lint-staged": {
		"*.{js,jsx}": "eslint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
